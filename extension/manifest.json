{
    "name": "Tell-Tale Tab - Rehash",
    "version": "2.0.1",
    "description": "This is a remake of favicon extension Tell-Tale Tab by chet.luther to repair the problems caused by the 2.0.0 release.",
    "icons": {
       "128": "icon_128.png",
       "48": "icon_48.png"
    },
    "background_page": "background.html",
    "content_scripts": [ {
        "js": [ "tellTaleTab.js" ],
        "matches": [
            "https://mail.google.com/mail/*",
            "https://mail.google.com/a/*",
            "http://www.google.com/reader/*",
            "https://wave.google.com/wave/*",
            "http://www.netvibes.com/",
            "http://www.facebook.com/*",
            "http://twitter.com/*",
            "https://www.google.com/voice",
			"https://www.google.com/voice?*"
        ]
    } ]
}
